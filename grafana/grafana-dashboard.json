        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "description": "Total CPU usage per namespace",
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "id": 1,
      "options": { "legend": { "displayMode": "table", "placement": "bottom" } },
      "targets": [
        {
          "expr": "sum(rate(container_cpu_usage_seconds_total{namespace!=\"\",container!=\"POD\"}[5m])) by (namespace)",
          "legendFormat": "{{namespace}}",
          "refId": "A"
        }
      ],
      "title": "CPU Usage by Namespace",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "description": "Total memory usage per namespace",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "id": 2,
      "targets": [
        {
          "expr": "sum(container_memory_working_set_bytes{namespace!=\"\",container!=\"POD\"}) by (namespace)",
          "legendFormat": "{{namespace}}",
          "refId": "A"
        }
      ],
      "title": "Memory Usage by Namespace",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "gridPos": { "h": 7, "w": 8, "x": 0, "y": 8 },
      "id": 3,
      "targets": [
        {
          "expr": "topk(10, sum(rate(container_cpu_usage_seconds_total{namespace!=\"\",container!=\"POD\"}[5m])) by (pod, namespace))",
          "legendFormat": "{{namespace}} / {{pod}}",
          "refId": "A"
        }
      ],
      "title": "Top 10 Pods by CPU",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "gridPos": { "h": 7, "w": 8, "x": 8, "y": 8 },
      "id": 4,
      "targets": [
        {
          "expr": "topk(10, sum(container_memory_working_set_bytes{namespace!=\"\",container!=\"POD\"}) by (pod, namespace))",
          "legendFormat": "{{namespace}} / {{pod}}",
          "refId": "A"
        }
      ],
      "title": "Top 10 Pods by Memory",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "gridPos": { "h": 7, "w": 8, "x": 16, "y": 8 },
      "id": 5,
      "targets": [
        {
          "expr": "sum(increase(kube_pod_container_status_restarts_total{namespace!=\"\"}[1h])) by (namespace)",
          "legendFormat": "{{namespace}}",
          "refId": "A"
        }
      ],
      "title": "Container Restarts (Last 1h)",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 15 },
      "id": 6,
      "targets": [
        {
          "expr": "sum(kube_node_status_condition{condition=\"Ready\",status=\"true\"})",
          "legendFormat": "Ready Nodes",
          "refId": "A"
        },
        {
          "expr": "sum(kube_node_status_condition{condition=\"Ready\",status=\"false\"})",
          "legendFormat": "Not Ready Nodes",
          "refId": "B"
        }
      ],
      "title": "Node Readiness",
      "type": "stat"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 15 },
      "id": 7,
      "targets": [
        {
          "expr": "100 - (avg(rate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
          "legendFormat": "Cluster CPU Used %",
          "refId": "A"
        }
      ],
      "title": "Cluster CPU Used %",
      "type": "gauge"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 22 },
      "id": 8,
      "targets": [
        {
          "expr": "(1 - (sum(node_memory_MemAvailable_bytes) / sum(node_memory_MemTotal_bytes))) * 100",
          "legendFormat": "Cluster Memory Used %",
          "refId": "A"
        }
      ],
      "title": "Cluster Memory Used %",
      "type": "gauge"
    },
    {
      "datasource": {
        "type": "loki",
        "uid": "${DS_LOKI}"
      },
      "gridPos": { "h": 14, "w": 24, "x": 0, "y": 29 },
      "id": 9,
      "targets": [
        {
          "expr": "{namespace=\"$namespace\"} |= \"$search\"",
          "refId": "A"
        }
      ],
      "title": "Kubernetes Logs (Loki) â€” Filter by Namespace",
      "type": "logs"
    }
  ],
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["kubernetes", "prometheus", "loki", "monitoring"],
  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS",
        "label": "Prometheus Datasource",
        "type": "datasource",
        "query": "prometheus",
        "refresh": 1
      },
      {
        "name": "DS_LOKI",
        "label": "Loki Datasource",
        "type": "datasource",
        "query": "loki",
        "refresh": 1
      },
      {
        "name": "namespace",
        "label": "Namespace",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": "label_values(kube_pod_info, namespace)",
        "refresh": 1,
        "includeAll": true,
        "multi": true
      },
      {
        "name": "search",
        "label": "Search",
        "type": "textbox",
        "query": "",
        "current": { "text": "", "value": "" }
      }
    ]
  },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": { "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"] },
  "timezone": "",
  "title": "Kubernetes Monitoring +_toggle Logs (Prometheus + Loki)",
